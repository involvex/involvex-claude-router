    "format:cli": "bun run --cwd cli format",
    "lint": "eslint .",
    "lint:cli": "bun run --cwd cli lint",
    "lint:fix": "eslint --fix .",
    "start": "next start",
    "typecheck:cli": "bun run --cwd cli typecheck"
  },
  "config": {
    "appName": "involvex-claude-router"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.7.0",
    "@xyflow/react": "^12.10.1",
    "bcryptjs": "^3.0.3",
    "better-sqlite3": "^12.6.2",
    "express": "^5.2.1",
    "fs": "^0.0.1-security",
