    "Readme.md",
    "cli/dist/index.js",
    "src",
    "open-sse",
    "package.json"
  ],
  "workspaces": [
    "cli"
  ],
  "scripts": {
    "prebuild": "bun run format && bun run lint:fix",
    "build": "next build --turbo && bun run build:cli",
