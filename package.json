{
  "name": "@involvex/claude-router",
  "version": "0.2.97",
  "private": true,
  "description": "Involvex's Claude Router Web dashboard",
  "repository": {
    "type": "git",
    "url": "https://github.com/involvex/involvex-claude-router.git"
  },
