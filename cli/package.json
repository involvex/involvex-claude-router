{
  "name": "@involvex/claude-router-cli",
  "version": "0.0.1",
  "description": "claude-router-cli",
  "license": "MIT",
  "author": "involvex",
  "type": "module",
  "bin": {
    "claude-router-cli": "dist/index.js"
  },
  "scripts": {
    "prebuild": " bun run lint:fix && bun run typecheck",
    "build": "bun build src/index.ts --outdir dist --target node",
    "dev": "bun run src/index.ts",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@types/kill-port": "^2.0.3",
    "kill-port": "^2.0.1"
  },
  "devDependencies": {
    "@types/bun": "^1.1.0",
    "@types/node": "^25.2.3",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "prettier": "^3.4.0",
    "prettier-plugin-organize-imports": "^4.0.0",
    "prettier-plugin-packagejson": "^2.0.0",
    "prettier-plugin-sort-imports": "^1.0.0",
    "typescript": "^5.7.0"
  }
}
